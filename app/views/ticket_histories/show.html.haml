- model_class = TicketHistory
.page-header
  %h1=t '.title', default: model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:staff_id) + ':'
  %br
  = @ticket_history.staff_id
%p
  %strong= model_class.human_attribute_name(:state) + ':'
  %br
  = @ticket_history.state
%p
  %strong= model_class.human_attribute_name(:resolution) + ':'
  %br
  = @ticket_history.resolution

.form-actions
  = link_to t('.back', default: t("helpers.links.back")), ticket_histories_path, class: 'btn'
  = link_to t('.edit', default: t("helpers.links.edit")), edit_ticket_history_path(@ticket_history), class: 'btn'
  = link_to t('.destroy', default: t("helpers.links.destroy")), ticket_history_path(@ticket_history), method: "delete", data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) }, class: 'btn btn-danger'
